{
    "name": "Prudle QA",
    "version": "1.0.3",
    "manifest_version": 2,
    "description": "Prudleâ€™s QA module significantly reduces manual effort, save upto 50% defect raising time and increases overall efficiency",
    "background": {
        "scripts": ["js/Background.js","js/lib/jquery.js"],
        "persistent": false
    },
    "browser_action": {
        "default_title": "Prudle QA",
        "default_icon": "images/icon38.png",
        "default_popup": "popup.html"
    },
    "icons": {
        "16": "images/icon16.png",
        "48": "images/icon48.png",
        "128": "images/icon128.png"
    },
    "permissions": ["tabs", "activeTab", "<all_urls>","storage"],
    "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'",
    "web_accessible_resources": ["images/*.png","js/lib/bootstrap.min.js","js/popup.js","js/modal.js"],
    "externally_connectable": {
        "matches": []
    }
}